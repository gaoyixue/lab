<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.5.dtd 

 

">
<struts>

	<package name="com" namespace="/" extends="struts-default">
        <action name="queryAction" class="com.queryAction">
            <result>/showTitle.jsp</result>
            <result name="error">/error.jsp</result>
        </action>
         <action name="showinfoAction" class="com.clicktitleAction">
            <result>/showinfo.jsp</result>
             <result name="error">/error.jsp</result>
        </action>
         <action name="deletebookAction" class="com.deletebookAction">
            <result>/allinfo.jsp</result>
             <result name="error">/error.jsp</result>
        </action>
         <action name="addbookAction" class="com.addbookAction">
            <result>/allinfo.jsp</result>
        </action>
        <action name="updatebookAction" class="com.updatebookAction">
            <result>/allinfo.jsp</result>
             <result name="error">/error.jsp</result>
        </action>
    </package>
    

</struts>
